    "# Test datapoints\n",
    "num_points = 100\n",
    "points_list = list()\n",
    "for _ in range(num_points):\n",
    "    p = random.randint(0,10)\n",
    "    if p<6:\n",
    "        point = (random.uniform(-4, 3), random.uniform(-4, 3))\n",
    "    elif p>=6 and p<8:\n",
    "        point = (random.uniform(3, 5), random.uniform(3, 5))\n",
    "    else:\n",
    "        point = (random.uniform(4, 7), random.uniform(4, 7))\n",
    "    points_list.append(point)"
   ]
  },
